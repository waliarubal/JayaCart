<?xml version="1.0" encoding="UTF-8" ?>
<ResourceDictionary
    x:Class="JayaCart.Shared.Styles.SharedStyles"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <OnPlatform x:Key="FontAwesomeSolid" x:TypeArguments="x:String">
        <On Platform="iOS" Value="Font Awesome 5 Free Solid" />
        <On Platform="Android" Value="fa-solid-900.ttf#Font Awesome 5 Free Solid" />
    </OnPlatform>
    <OnPlatform x:Key="FontAwesomeRegular" x:TypeArguments="x:String">
        <On Platform="iOS" Value="Font Awesome 5 Free Regular" />
        <On Platform="Android" Value="fa-regular-400.ttf#Font Awesome 5 Free Regular" />
    </OnPlatform>
    <OnPlatform x:Key="FontAwesomeBrands" x:TypeArguments="x:String">
        <On Platform="iOS" Value="Font Awesome 5 Brands Regular" />
        <On Platform="Android" Value="fa-brands-400.ttf#Font Awesome 5 Brands Regular" />
    </OnPlatform>
</ResourceDictionary>